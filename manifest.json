{
    "manifest_version": 3,
    "name": "X Color Fixer",
    "version": "1.0",
    "description": "Reverts X premium panel to original colors",
    "icons": {
      "16": "x-color-correct.png",
      "48": "x-color-correct.png",
      "128": "x-color-correct.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.x.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }